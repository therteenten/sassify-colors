{
  "name": "@therteenten/sassify-colors",
  "version": "1.0.0",
  "description": "Большой набор цветовых палитр, предоставляемых в виде переменных на языке CSS-препроцессора @sass",
  "main": "index.scss",
  "scripts": {
    "start": "npm run serve",
    "serve": "sass tests/index.test.scss:tests/index.test.css --no-source-map --watch && npm run docs",
    "test": "sass tests/index.test.scss:tests/index.test.css --no-source-map && npm run docs",
    "publish": "npm publish --access public",
    "docs": "npx sassdoc . -d docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therteenten/sassify-colors.git"
  },
  "keywords": [
    "therteenten",
    "sassify",
    "sass",
    "sass-mixins",
    "sass-functions",
    "sass-library",
    "scss",
    "scss-mixins",
    "scss-functions",
    "scss-library",
    "dart-sass"
  ],
  "author": "Haba Kudzaev (therteenten)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/therteenten/sassify-colors/issues"
  },
  "homepage": "https://github.com/therteenten/sassify-colors#readme",
  "devDependencies": {
    "sass": "^1.69.4"
  }
}
